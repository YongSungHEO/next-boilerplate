{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint --fix",
    "format": "prettier --write ./src",
    "analyze": "ANALYZE=true npm run build",
    "postbuild": "next-sitemap",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@sentry/nextjs": "^7.86.0",
    "antd": "^5.12.2",
    "antd-mobile": "^5.33.0",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "next": "14.0.4",
    "next-translate": "^2.6.2",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "next-translate-plugin": "^2.6.2",
    "sass": "^1.69.5",
    "typescript": "^5"
  }
}
